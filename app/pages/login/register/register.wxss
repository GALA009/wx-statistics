/* pages/login/register/register.wxss */
.page{
  position: fixed;
  width: 100%;
  height: 100%;
  background: #F2F4F5;
}
.weui-cells__title{
  margin-top: 150rpx;
  margin-bottom: 80rpx;
  color: #000000;
  font-size: 40rpx;
  text-align: center;
}
.my-up-content{
  overflow: hidden
}
.my-up-content .weui-cell {
  background:#ffffff;
  font-size:30rpx;
  padding:20rpx;
}
.weui-cell__hd{
  position: relative;
  width: 160rpx;
}
.phoneText{
  height:2.58823529em;
  min-height:2.58823529em;
  line-height:2.58823529em;

}
.weui-cell__bd_in-select-before,.weui-cell__hd_in-select-after {
  padding-left: 20rpx;
}
.my-bk-btn {
  margin:60rpx 40rpx 0;
}
button{
  background: #4B4A5A;
  color: #fff;
}
button[disabled]:not([type]){
  background: #A1A2A1;
  color: #fff;
}
.getCode{
  position: absolute;
  top: 24rpx;
  right: 20rpx;
  color: #fff;
  font-size: 28rpx;
  z-index: 9;
  font-weight: 300;
}